{
  "compilerOptions": {
    "outDir": "./static/./dist",
    "target": "es2016",
    "declaration": false,
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
   "include": [
      "./static/**/*"
      ],
  
} 
